<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/white"
    android:background="@color/white"
    tools:context=".cadastro">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="292dp"
        android:layout_height="40dp"
        android:text="Cadastre - se"
        android:textColor="@color/main"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.403"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.076" />

    <TextView
        android:id="@+id/textViewLogin"
        android:layout_width="84dp"
        android:layout_height="21dp"
        android:text="Login"

        android:textColor="@color/main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/EditTextSenha"
        android:layout_width="315dp"
        android:layout_height="41dp"
        android:layout_marginTop="272dp"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/EditTextCPF"
        android:layout_width="315dp"
        android:layout_height="41dp"
        android:layout_marginTop="344dp"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/EditTextNome"
        android:layout_width="315dp"
        android:layout_height="41dp"
        android:layout_marginTop="132dp"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/EditTextEmail"
        android:layout_width="315dp"
        android:layout_height="41dp"
        android:layout_marginTop="204dp"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <EditText
        android:id="@+id/EditTexttelefone"
        android:layout_width="315dp"
        android:layout_height="41dp"
        android:layout_marginTop="412dp"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/EditTextData"
        android:layout_width="315dp"
        android:layout_height="41dp"
        android:layout_marginTop="480dp"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="Senha"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextSenha"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="Email"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextEmail"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="Nome"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextNome"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewCPF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="CPF"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextCPF"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewTelefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="Telefone"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/EditTexttelefone"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="Data de Nascimento"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextData"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="315dp"
        android:layout_height="53dp"
        android:backgroundTint="@color/main"
        android:text="Cadastrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditTextData"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="116dp"
        android:text="Tem uma conta?"
        app:layout_constraintBottom_toBottomOf="@+id/textViewLogin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintVertical_bias="0.996" />

</androidx.constraintlayout.widget.ConstraintLayout>